{
  "dependencies": {
    "@welovecoding/backend": "./backend",
    "@welovecoding/frontend": "./frontend",
    "fs-extra": "3.0.1"
  },
  "description": "We Love Coding provides a numerous collection of video tutorials for developers.",
  "devDependencies": {
    "concurrently": "3.5.0",
    "husky": "0.14.3"
  },
  "engines": {
    "node": "8.1.3"
  },
  "name": "@welovecoding/welovecoding",
  "private": true,
  "repository": {
    "type": "git",
